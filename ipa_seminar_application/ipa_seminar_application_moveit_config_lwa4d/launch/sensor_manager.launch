<launch>

  <!-- This file makes it easy to include the settings for sensor managers -->  

  <!-- Load the robot specific sensor manager; this sets the moveit_sensor_manager ROS parameter -->
  <arg name="moveit_sensor_manager" default="lwa4d" />
  <include file="$(find ipa_seminar_application_moveit_config_lwa4d)/launch/$(arg moveit_sensor_manager)_moveit_sensor_manager.launch" />
  
</launch>
